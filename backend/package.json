  "scripts": {
    "start": "export NODE_CONFIG_DIR=./backend/config && node backend/index.js --with-mock",
    "start:debug": "export NODE_CONFIG_DIR=./backend/config && DEBUG=express:* node backend/index.js --with-mock",
    "start:prod": "export NODE_CONFIG_DIR=./backend/config NODE_ENV=production && node backend/index.js --prod",
    "start:stage": "export NODE_CONFIG_DIR=./backend/config NODE_ENV=staging && node backend/index.js --stage",
    "build": "webpack --progress --config frontend/webpack.dev.js",
    "build:prod": "webpack --progress --config frontend/webpack.prod.js",
    "test:api": "export NODE_CONFIG_DIR=./backend/config && node ./backend/API/tests/test.executor.js --all",
    "test:api:latest": "export NODE_CONFIG_DIR=./backend/config && node ./backend/API/tests/test.executor.js -v 1"
  },
